   "source": [
    "def mySqrt(x, err=1e-12):\n",
    "    y = x\n",
    "    while abs(y*y - x) >= err:\n",
    "        y = (y + x/y)/2\n",
    "    return y\n",
    "\n",
    "\n",
    "print(mySqrt(4))\n",
    "print(mySqrt(8))\n",
    "print(mySqrt(9))\n",
    "print(mySqrt(3))\n",
    "print(mySqrt(2))\n"
   ]
  }
