    "def singleNumber(nums):\n",
    "    nums.sort()\n",
    "\n",
    "    for i in range(0, len(nums) - 1, 2):\n",
    "        if nums[i] != nums[i + 1]:\n",
    "            return nums[i]\n",
    "    return nums[len(nums) - 1]\n",
    "\n",
    "\n",
    "print(singleNumber([2,2,1]))\n",
    "print(singleNumber([4,1,2,1,2]))\n",
    "print(singleNumber([2,2,1]))"
   ]
  }
