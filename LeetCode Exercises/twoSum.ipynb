    "def twoSum(nums, target):\n",
    "    for i in range(len(nums) - 1):\n",
    "        for j in range(1, len(nums) - i):\n",
    "            if nums[i] + nums[j+i] == target:\n",
    "                return [i, j+i]\n",
    "\n",
    "print(twoSum(nums = [3,2,4], target = 6))\n",
    "print(twoSum(nums = [2,7,11,15], target = 9))"
   ]
  }
