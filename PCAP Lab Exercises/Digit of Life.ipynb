    "def digitOfLife(x):\n",
    "    if len(x) != 8:\n",
    "        pass\n",
    "    else:\n",
    "        digit = int(x)\n",
    "        while digit > 9:\n",
    "            total = 0\n",
    "            for num in str(digit):\n",
    "                total += int(num)\n",
    "            digit = total\n",
    "        return digit\n",
    "\n",
    "\n",
    "print(digitOfLife(\"19991229\"))"
   ]
  }
