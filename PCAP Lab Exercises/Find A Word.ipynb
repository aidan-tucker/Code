    "def pos(x,y):\n",
    "    count = 0\n",
    "    for char in y:\n",
    "        if char.lower() == x[count].lower(): count += 1\n",
    "        if count == len(x): return True\n",
    "    return False\n",
    "\n",
    "\n",
    "print(pos(\"donor\", \"Nabucodonosor\"))\n",
    "print(pos(\"donut\", \"Nabucodonosor\"))"
   ]
  }
